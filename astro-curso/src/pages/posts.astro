---
const response = await fetch("https://jsonplaceholder.typicode.com/photos");
const posts = await response.json();
---

<h1>Mis publicaciones</h1>
{
    posts.map((post) => (
        <div>
            <h1>{post.id}</h1>
            <h2>{post.title}</h2>
            <p>{post.url}</p>
        </div>
    ))
}
<style>
    body {
        background-color: #2e2e2e;
        color: white;
        font-family: "Montserrat";
    }
</style>
